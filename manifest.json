{
  "manifest_version": 2,

  "name": "weChatSpider",
  "description": "weChatSpider",
  "version": "1.0",

  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
     "http://*/"
  ],
  "content_scripts": [
        {
            "matches": ["http://weixin.sogou.com/*","http://mp.weixin.qq.com/*"],
            "js": ["./js/jquery.js","./js/jquery.cookie.js","./js/uu.js"]
        }
  ]
}
